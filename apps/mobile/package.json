{
  "name": "@creator-mode/mobile",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .ts,.tsx",
    "pods": "cd ios && pod install",
    "build": "npm run build:ios && npm run build:android",
    "build:ios": "react-native bundle --platform ios --entry-file index.js --bundle-output ios/main.jsbundle --assets-dest ios --dev false",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:android:clean": "cd android && ./gradlew clean && ./gradlew assembleRelease"
  },
  "dependencies": {
    "@creator-mode/policy-engine": "*",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@supabase/supabase-js": "^2.39.0",
    "@tanstack/react-query": "^5.17.0",
    "expo": "~51.0.0",
    "expo-dev-client": "~4.0.0",
    "expo-status-bar": "~1.12.0",
    "react": "18.2.0",
    "react-native": "0.74.0",
    "react-native-mmkv": "^2.11.0",
    "react-native-safe-area-context": "^4.8.0",
    "react-native-screens": "^3.29.0",
    "react-native-webview": "^13.6.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/runtime": "^7.23.0",
    "@expo/cli": "^0.16.0",
    "@react-native/babel-preset": "^0.73.0",
    "@react-native/eslint-config": "^0.73.0",
    "@react-native/metro-config": "^0.73.0",
    "@react-native/typescript-config": "^0.73.0",
    "@types/react": "^18.2.0",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.7.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-preset-expo": "~10.0.0",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18"
  }
}
