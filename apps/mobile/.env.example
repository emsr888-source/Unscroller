# Unscroller Mobile App - Environment Variables Example
# Copy this file to .env and fill in your values

# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
EXPO_PUBLIC_SUPABASE_KEY=your-anon-key-here

# Backend API
EXPO_PUBLIC_API_URL=https://api.unscroller.app

# For local development on physical device, use your computer's IP:
# EXPO_PUBLIC_DEV_API_URL=http://192.168.1.100:3000

# Feature Flags
EXPO_PUBLIC_ENABLE_AI=true
EXPO_PUBLIC_ENABLE_ANALYTICS=true

# Debug Mode (development only)
EXPO_PUBLIC_DEBUG=false
